 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na); // 1 0
    Not(in=b, out=nb); // 1 0
    Nand(a=na, b=nb, out=out); // 0 0 0 1
}
